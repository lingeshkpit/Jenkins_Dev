pipeline {
    agent any
    
    stages {
    
      stage ('MVN_clean') {
            steps {
                sh '''
                  'cd /var/lib/jenkins/workspace/maven_pipe'
                  ' mvn clean'
                  '''    
            }
            }
      stage ('MVN_build ') {
            steps {
                sh 'mvn archetype:generate -DgroupId=com.mycompany.app -DartifactId=my-app -DarchetypeArtifactId=maven-archetype-quickstart -DarchetypeVersion=1.4 -DinteractiveMode=false' 
            }
            }
        stage('Docker') {
            steps {
                sh 'docker version'
            }
        }
        stage('Docker_RMI') {
            steps {
                sh 'docker rmi $(docker images -q)'
            }
          }
        stage('Docker_images') {
            steps {
                sh 'docker images'
            }
        }
        stage('Docker_pull') {
            steps {
                sh 'docker pull bitnami/jenkins:latest'
            }
        }
        
        stage('Build docker image') {
            steps {
                sh 'docker build -t devops .'
            }
        }
        
     }
      }
